{
  "name": "appStarter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "jest",
    "start": "react-native start",
    "ios": "react-native run-ios",
    "ra": "react-native run-android",
    "android": "react-native run-android",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "node ./scripts/silence-recycled-files-warning.js",
    "rca": "cd ./android && ./gradlew clean && ./gradlew uninstallAll && cd .. && react-native run-android",
    "apk": "cd ./android && ./gradlew clean && ./gradlew assembleRelease --stacktrace && open app/build/outputs/apk/release && cd ..",
    "aab": "cd ./android && ./gradlew clean && ./gradlew bundleRelease --stacktrace && open app/build/outputs/bundle/release && cd ..",
    "apk:install": "cd ./android && ./gradlew clean && ./gradlew uninstallAll && ./gradlew installRelease --stacktrace && open app/build/outputs/apk/release && cd .."
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.3",
    "i18n-js": "^3.5.0",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-localize": "^1.3.1",
    "react-native-modalfy": "^1.0.2",
    "react-native-reanimated": "1.4.0",
    "react-native-svg": "^9.13.3",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.4",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/jest": "^24.0.24",
    "@types/react": "^16.9.15",
    "@types/react-native": "^0.60.25",
    "@types/react-redux": "^7.1.5",
    "@types/react-test-renderer": "16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-native": "^3.8.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "prettier": "1.19.1",
    "react-test-renderer": "16.9.0",
    "typescript": "^3.7.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
